    "#fibonacci series\n",
    "n=int(input(\"enter a number:\"))\n",
    "first=1\n",
    "second=0\n",
    "r=0\n",
    "i=1\n",
    "while(i<=n):\n",
    "      r=first+second\n",
    "      print(r)\n",
    "      first=second\n",
    "      second=r\n",
    "      i=i+1"
